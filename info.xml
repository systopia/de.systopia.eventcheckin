<?xml version="1.0"?>
<extension key="de.systopia.eventcheckin" type="module">
  <file>eventcheckin</file>
  <name>Event Check-In</name>
  <description>Provides convenient check-in functions like QR codes</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.eventcheckin</url>
    <url desc="Documentation">https://docs.civicrm.org/eventcheckin</url>
    <url desc="Support">https://github.com/systopia/de.systopia.eventcheckin/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>1.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <requires>
    <ext>de.systopia.remotetools</ext>
  </requires>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Eventcheckin</namespace>
  </civix>
</extension>
